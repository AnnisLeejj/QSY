<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/layout"
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:background="@drawable/personal_center">

        <TextView
            android:id="@+id/login_button"
            android:layout_width="75dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="21dp"
            android:background="@drawable/login_bn"
            android:gravity="center"
            android:text="登录"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/zhu_ti"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/image_back"
            android:layout_marginRight="15dp"
            android:background="?attr/theme_back"
            android:visibility="gone" />

        <TextView
            android:id="@+id/image_back"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignLeft="@+id/login_button"
            android:layout_centerVertical="true"
            android:background="#9ddcf6"
            android:gravity="center" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/myself_share" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/share_button"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                android:drawableRight="@drawable/jt"
                android:gravity="center|left"
                android:text="应用分享"
                android:textColor="#333333"
                android:textSize="16dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#40000000" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/myself_delete" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/remove_button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="12dp"
                android:gravity="center|left"
                android:text="清除缓存"
                android:textColor="#333333"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/hucun_button"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_alignBaseline="@+id/remove_button"
                android:layout_alignBottom="@+id/remove_button"
                android:layout_alignRight="@+id/remove_button"
                android:layout_marginRight="5dp"
                android:gravity="center|left"
                android:text=""
                android:textColor="#fb5747"
                android:textSize="16dp" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/pw_Sever"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/myself_lock" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/pa_button"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginRight="12dp"
                    android:layout_weight="1"
                    android:drawableRight="@drawable/jt"
                    android:gravity="center|left"
                    android:text="密码服务"
                    android:textColor="#333333"
                    android:textSize="16dp" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/myself_update" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/ban_ben_gen_xin"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginRight="12dp"
                    android:layout_weight="1"
                    android:drawableRight="@drawable/jt"
                    android:gravity="center|left"
                    android:text="版本更新"
                    android:textColor="#333333"
                    android:textSize="16dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="#40000000" />
            </LinearLayout>

            <TextView
                android:id="@+id/getVerName_button"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:gravity="center|left"
                android:textColor="?attr/personal_center_text_color"
                android:textSize="16dp"
                android:visibility="gone" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="gone">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/yhpj_pj" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/score_button"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:drawableRight="@drawable/jt"
                android:gravity="center|left"
                android:text="用户评分"
                android:textColor="?attr/personal_center_text_color"
                android:textSize="16dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#40000000" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/myself_write" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/feedback_button"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                android:drawableRight="@drawable/jt"
                android:gravity="center|left"
                android:text="意见反馈"
                android:textColor="#333333"
                android:textSize="16dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#40000000" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/myself_about" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/about_button"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                android:drawableRight="@drawable/jt"
                android:gravity="center|left"
                android:text="关于我们"
                android:textColor="#333333"
                android:textSize="16dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#40000000" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/exit_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:layout_weight="1"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="gone">

        <TextView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_margin="6dp"
            android:background="@drawable/myself_about" />

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/exit_button"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginRight="12dp"
                android:layout_weight="1"
                android:drawableRight="@drawable/jt"
                android:gravity="center|left"
                android:text="退出账户"
                android:textColor="#333333"
                android:textSize="16dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#40000000" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/return_button"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:background="#ffffff"
        android:gravity="center"
        android:text="返回主页"
        android:textColor="?attr/personal_center_text_color"
        android:visibility="gone" />

</LinearLayout>