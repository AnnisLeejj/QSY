<?xml version="1.0" encoding="utf-8"?>
<ScrollView 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4f4f4"
    >
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4f4f4"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="?attr/appheadbackground" >

        <TextView
            android:id="@+id/textview01"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="投诉举报"
            android:textColor="#ffffff"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/textview"
            android:layout_width="30dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_gravity="center"
            android:drawableRight="@drawable/return_img_view"
            android:gravity="center"
            android:textSize="16dp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#f3f3f3"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:gravity="center|left"
            android:text="药监局投诉举报电话:"
            android:textColor="#000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center|left"
            android:text="12331"
            android:textSize="22dp"
            android:textColor="#f13f16" />

        <TextView
            android:id="@+id/Text_12331_text"
            android:layout_width="30dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:background="@drawable/bohaopan"
            android:gravity="center|left"
            android:textColor="#f13f16" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_margin="10dp"
        android:gravity="center"
        android:orientation="horizontal" >

        <EditText
            android:id="@+id/phose_your"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/back_img"
            android:gravity="center"
            android:hint="您的电话"
            android:textColor="#000000" />
        
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_margin="10dp"
        android:gravity="center"
        android:orientation="horizontal" >
<EditText
            android:id="@+id/phose_your_name"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/back_img"
            android:gravity="center"
            android:hint="您的姓名"
            android:textColor="#000000" />
        
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_margin="10dp"
        android:gravity="center"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/mCategory"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/back_img"
            android:gravity="center"
            android:text="选择企业"
            android:textColor="#000000" />
    </LinearLayout>
<TextView 
    android:layout_width="match_parent"
    android:layout_height="35dp"
    android:text="举报内容:"
    android:gravity="bottom"
    android:layout_marginLeft="10dp"
    android:textColor="#000000"
    />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="120dp"
     android:layout_margin="10dp"
    android:background="@drawable/back_rectangular"
    android:gravity="center"
    android:orientation="vertical" >

    <EditText
        android:id="@+id/context_complaint"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:textColor="#000000"
        android:background="@drawable/edittext_back_rectangular" />

</LinearLayout>
   <LinearLayout 
         android:layout_width="match_parent"
    android:layout_height="20dp"
    android:gravity="center"
    android:orientation="vertical"
    android:background="#b7e9f5"
       >
        <TextView 
    android:layout_width="match_parent"
    android:layout_height="20dp"
    android:text="附件:"
    android:layout_margin="10dp"
    android:gravity="center|left"
    android:textColor="#000000"
    />
     
   </LinearLayout>
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="120dp"
    android:orientation="vertical"
    >
        <TextView
        android:id="@+id/add_fujian_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:gravity="center|left"
        android:text="添加附件" 
        android:textColor="#000000"
        />
<ListView 
    android:id="@+id/fujian_list"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:gravity="center|left"
    />
    
</LinearLayout>
    <TextView 
        android:id="@+id/ti_jiao_Complaint_reporting"
        android:layout_width="match_parent"
        android:text="提交"
        android:layout_margin="10dp"
        android:textSize="17dp"
        android:gravity="center"
        android:layout_height="45dp"
        android:textColor="#ffffff"
        android:background="@drawable/complaint_back_rectangular"
        
        />
</LinearLayout>
</ScrollView>


